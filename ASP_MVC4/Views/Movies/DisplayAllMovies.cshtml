@model ASP_MVC4.Models.ViewModel.MovieViewModel
@{
    ViewBag.Title = "DisplayAllMovies";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Movies</h2>

<table class="table table-bordered table-hover">
    <tr>
        <th>Movie</th>
    </tr>
    @foreach (var movie in Model.Movies)
    {
        <tr>
            <td>@movie.Name</td>
        </tr>
    }
</table>

